e Binding</h2>\n        <hr>\n\n        <h4><u>Interpolación / v-text / v-html</u></h4>\n\n        <b>{{ 3 + 4 }}</b>\n        <div>{{ mensaje }}</div>\n        <div v-text=\"mensaje\"></div>\n        <div v-html=\"mensaje\"></div>\n        <br>\n        <div>{{ mensajeHTML }}</div>\n        <div v-text=\"mensajeHTML\"></div>\n        <div v-html=\"mensajeHTML\"></div>\n        <hr>\n\n        <!-- -------------------------------------------------------------------------- -->\n        <h4><u>Property Binding / v-bind</u></h4>\n        <input type=\"text\" value=\"123\"> <br>\n        <input type=\"text\" value=\"valor\"> <br>\n        <input type=\"text\" v-bind:value=\"valor\"> {{ valor }} <br>\n        <input type=\"text\" :value=\"valor\"> {{ valor }} <br>\n        <hr>\n\n        <!-- -------------------------------------------------------------------------- -->\n        <h4><u>Event Binding / v-on</u></h4>\n        <button class=\"btn btn-success my-3 mr-2\" v-on:click=\"contador++\">Contador: {{ contador }}</button>\n        <button class=\"btn btn-warning my-3 mr-2\" @click=\"contador2=contador2+1\">\n            Contador: {{ contador2 }}\n        </button>\n        <button class=\"btn btn-danger my-3\" @click=\"incrementar()\">\n            Contador: {{ getContador() }}\n        </button>\n        <hr>\n\n        <!-- -------------------------------------------------------------------------- -->\n        <h4><u>Doble Data Binding / v-model</u></h4>\n        <h5>Manual</h5>\n        <input type=\"text\" :value=\"valor2\" @input=\"actualizar($event)\"> {{ valor2 }} <br>\n        <br>\n\n        <h5>Automática</h5>\n        <input type=\"text\" v-model=\"valor3\"> {{ valor3 }} <br>\n    </div>        \n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'HelloWorld',\n  /* props: {\n    msg: String\n  } */\n  data() {\n    return {\n        mensaje: 'Soy un mensaje',  \n        mensajeHTML: '<i>Soy un mensaje</i>',\n        valor: 123,\n        valor2: 456,\n        valor3: 789,\n        contador: 456,\n        contador2: 678,\n        contador3: 789,\n    }\n  },\n  methods : {\n      incrementar() {\n          this.contador3++\n      },\n      getContador() {\n          return this.contador3\n      },\n      actualizar(e) {\n          //console.log('actualizar',e)\n          let dato = e.target.value   \n          this.valor2 = dato\n      }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n  .jumbotron {\n    background-color: purple;\n    color: white;\n  }\n\n  hr {\n    background-color: #eee;\n  }\n\n</style>\n"],"sourceRoot":"src/componentes"}]}